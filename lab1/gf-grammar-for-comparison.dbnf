NP_poss ::= NP_gen
NP_poss ::= Pron_poss

NP_gen ::= Det? N_gen # det head
NP_gen ::= PN_gen

-- some very special rules ---
NP        ::= Det CN_quoted # det head
CN_quoted ::= quote CN quote # punct head punct
